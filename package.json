{
  "name": "Ceres",
  "license": "AGPL-3.0",
  "version": "4.4.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/plentymarkets/plugin-ceres.git"
  },
  "scripts": {
    "start": "webpack --watch --progress",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --progress",
    "build:prod": "webpack --env.prod --progress",
    "build:js": "npm run build:js:dev && npm run build:js:prod",
    "build:js:dev": "webpack --config-name scripts --progress",
    "build:js:prod": "webpack --config-name scripts --env.prod --progress",
    "build:sass": "npm run build:sass:dev && npm run build:sass:prod",
    "build:sass:dev": "webpack --config-name styles --progress",
    "build:sass:prod": "webpack --config-name styles --env.prod --progress",
    "build:sass-vendor": "node tools/bundleSass.js",
    "build:widgets": "node tools/collectWidgets.js",
    "prebuild": "npm run build:sass-vendor",
    "postbuild": "npm run build:widgets",
    "compareTranslations": "node tools/compareTranslations.js",
    "test": "npm run test:monetaryFormatter",
    "test:monetaryFormatter": "node -r esm tools/testMonetaryFormatter.js"
  },
  "browserslist": [
    "> .25%"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^8.6.5",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "del": "^2.2.0",
    "eslint": "^6.2.1",
    "eslint-config-google": "^0.13.0",
    "eslint-loader": "^2.2.1",
    "esm": "^3.2.25",
    "expose-loader": "^0.7.5",
    "glob": "^7.0.3",
    "jquery-lazyload": "^1.9.7",
    "mini-css-extract-plugin": "^0.8.0",
    "moment-locales-webpack-plugin": "^1.1.0",
    "node-sass": "^4.12.0",
    "postcss": "^6.0.21",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^1.0.5",
    "q": "^1.5.1",
    "sass-loader": "^7.3.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-fix-style-only-entries": "^0.3.0"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "core-js": "^3.2.1",
    "custom-event-polyfill": "^0.3.0",
    "detect-browser": "^2.3.0",
    "flag-icon-css": "^2.9.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.4.0",
    "lightbox2": "^2.11.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "owl.carousel": "2.2.0",
    "popper.js": "^1.15.0",
    "vue": "^2.6.10",
    "vue-script2": "2.0.3",
    "vuex": "^3.1.0"
  }
}
